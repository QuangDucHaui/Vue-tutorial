{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "noUnusedLocals": false,      // Bỏ qua cảnh báo biến không sử dụng
    "noUnusedParameters": false   // Bỏ qua cảnh báo tham số không sử dụng
  }
}
